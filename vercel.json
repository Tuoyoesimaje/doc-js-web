{
  "buildCommand": "npm install --prefix app && npm run build --prefix app",
  "outputDirectory": "app/dist",
  "installCommand": "echo 'Skipping root install'",
  "rewrites": [
    {
      "source": "/app/:path*",
      "destination": "/app/:path*"
    },
    {
      "source": "/app",
      "destination": "/app/index.html"
    }
  ]
}
