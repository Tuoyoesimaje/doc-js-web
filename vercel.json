{
  "buildCommand": "cd app && npm install && npm run build",
  "outputDirectory": "app/dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/app/:path*",
      "destination": "/app/:path*"
    },
    {
      "source": "/app",
      "destination": "/app/index.html"
    }
  ],
  "headers": [
    {
      "source": "/app/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
